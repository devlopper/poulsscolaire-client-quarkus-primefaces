<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns="https://jakarta.ee/xml/ns/jakartaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-facesconfig_4_0.xsd"
  version="4.0">

  <name>form</name>

  <ordering>
    <after>
      <name>omnifaces</name>
    </after>
  </ordering>

  <navigation-rule>
    <navigation-case>
      <description>page d'accueil</description>
      <from-outcome>indexPage</from-outcome>
      <to-view-id>/private/index.jsf</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- Session -->
    
    <navigation-case>
      <description>page de configuration de session</description>
      <from-outcome>sessionConfigurePage</from-outcome>
      <to-view-id>/private/__session__/configure.jsf</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- Deadline group -->
    
    <navigation-case>
      <description>page liste des groupes d'échéances</description>
      <from-outcome>deadlineGroupListPage</from-outcome>
      <to-view-id>/private/deadlinegroup/administrator/list.jsf</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- Deadline -->
    
    <navigation-case>
      <description>page liste des échéances</description>
      <from-outcome>deadlineListPage</from-outcome>
      <to-view-id>/private/deadline/administrator/list.jsf</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- Fee category -->
    
    <navigation-case>
      <description>page liste des rubriques</description>
      <from-outcome>feeCategoryListPage</from-outcome>
      <to-view-id>/private/feecategory/administrator/list.jsf</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- Fee -->
    
    <navigation-case>
      <description>page liste des frais</description>
      <from-outcome>feeListPage</from-outcome>
      <to-view-id>/private/fee/administrator/list/index.jsf</to-view-id>
      <redirect />
    </navigation-case>
    
    <navigation-case>
      <description>page de lecture de frais</description>
      <from-outcome>feeReadPage</from-outcome>
      <to-view-id>/private/fee/administrator/read/index.jsf</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- Adjusted fee -->
    
    <navigation-case>
      <description>page liste des frais ajustés</description>
      <from-outcome>adjustedFeeListPage</from-outcome>
      <to-view-id>/private/adjustedfee/administrator/list/index.jsf</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- Amount -->
    
    <navigation-case>
      <description>page liste des montants</description>
      <from-outcome>amountListPage</from-outcome>
      <to-view-id>/private/amount/administrator/list.jsf</to-view-id>
      <redirect />
    </navigation-case>
    
    <navigation-case>
      <description>page liste des échéances de montants</description>
      <from-outcome>amountDeadlineListPage</from-outcome>
      <to-view-id>/private/amountdeadline/administrator/list/index.jsf</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- Assignment Type -->
    
    <navigation-case>
      <description>page liste des types d'affectation</description>
      <from-outcome>assignmentTypeListPage</from-outcome>
      <to-view-id>/private/assignmenttype/administrator/list.jsf</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- Seniority -->
    
    <navigation-case>
      <description>page liste des anciennetés</description>
      <from-outcome>seniorityListPage</from-outcome>
      <to-view-id>/private/seniority/administrator/list.jsf</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- Gender -->
    
    <navigation-case>
      <description>page liste des genres</description>
      <from-outcome>genderListPage</from-outcome>
      <to-view-id>/private/gender/administrator/list.jsf</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- Student -->
    
    <navigation-case>
      <description>page liste des élèves</description>
      <from-outcome>studentListPage</from-outcome>
      <to-view-id>/private/student/administrator/list.jsf</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- School -->
    
    <navigation-case>
      <description>page liste des écoles</description>
      <from-outcome>schoolListPage</from-outcome>
      <to-view-id>/private/school/accountant/list.xhtml</to-view-id>
      <redirect />
    </navigation-case>
    
    <navigation-case>
      <description>page lecture d'école</description>
      <from-outcome>schoolReadPage</from-outcome>
      <to-view-id>/private/school/accountant/read/index.xhtml</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- Schooling -->
    
    <navigation-case>
      <description>page liste des scolarités</description>
      <from-outcome>schoolingListPage</from-outcome>
      <to-view-id>/private/schooling/accountant/list.xhtml</to-view-id>
      <redirect />
    </navigation-case>
    
    <navigation-case>
      <description>page lecture de scolarité</description>
      <from-outcome>schoolingReadPage</from-outcome>
      <to-view-id>/private/schooling/accountant/read/index.xhtml</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- Registration -->
    
    <navigation-case>
      <description>page liste des inscriptions</description>
      <from-outcome>registrationListPage</from-outcome>
      <to-view-id>/private/registration/accountant/list.xhtml</to-view-id>
      <redirect />
    </navigation-case>
    
    <navigation-case>
      <description>page lecture d'inscriptions</description>
      <from-outcome>registrationReadPage</from-outcome>
      <to-view-id>/private/registration/accountant/read/index.xhtml</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- Cash Register -->
    
    <navigation-case>
      <description>page liste des caisses enregistreuses</description>
      <from-outcome>cashRegisterListPage</from-outcome>
      <to-view-id>/private/cashregister/administrator/list.jsf</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- Payment Mode -->
    
    <navigation-case>
      <description>page liste des modes de paiement</description>
      <from-outcome>paymentModeListPage</from-outcome>
      <to-view-id>/private/paymentmode/administrator/list.jsf</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- Payment -->
    
    <navigation-case>
      <description>page liste des paiements</description>
      <from-outcome>paymentListPage</from-outcome>
      <to-view-id>/private/payment/administrator/list.jsf</to-view-id>
      <redirect />
    </navigation-case>
    
    <!-- Payment Adjusted Fee -->
    
    <navigation-case>
      <description>page liste des paiements de frais ajusté</description>
      <from-outcome>paymentAdjustedFeeListPage</from-outcome>
      <to-view-id>/private/paymentadjustedfee/administrator/list.jsf</to-view-id>
      <redirect />
    </navigation-case>
  </navigation-rule>
</faces-config>